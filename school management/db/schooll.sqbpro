CREATE TABLE Xull (
    id INTEGER PRIMARY KEY AUTOINCREMENT, -- Unique identifier for each record
    name TEXT NOT NULL,                     -- Name of the school
    address TEXT NOT NULL,                  -- Address of the school
    logo BLOB NOT NULL,                     -- Logo image stored as a BLOB
    phone TEXT NOT NULL,                    -- Phone number of the school
    email TEXT,                             -- Email of the school (can be NULL)
    stamp BLOB NOT NULL,                    -- School stamp image stored as a BLOB
    headTeacher TEXT NOT NULL,              -- Name of the head teacher
    HDSign BLOB NOT NULL                     -- Head teacher's signature image stored as a BLOB
);

-- Create the formClass table
CREATE TABLE formClass (
    id INTEGER PRIMARY KEY AUTOINCREMENT, -- Unique identifier for each record
    name TEXT NOT NULL,                     -- Name of the class
    academicYr TEXT NOT NULL,               -- Academic year (e.g., 2024-2025)
    term TEXT NOT NULL,                     -- Term (e.g., Term 1, Term 2)
    formTeacher TEXT NOT NULL,              -- Name of the form teacher
    FTSign BLOB NOT NULL                    -- Form teacher's signature image stored as a BLOB
);
</sql><current_tab id="0"/></tab_sql></sqlb_project>
